<?xml version="1.0"?>
<configuration>
<configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
        <section name="XoKeyHostApp.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
    </sectionGroup>
</configSections>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup>
  <runtime>


    <!-- .NET4 - enable legacy behaviour: if unmanaged (memory corrupted!) exception, then do NOT bomb out the application! 

    instead .NET catch will kick in as normal (and we should log + exit ASAP) 

    -->
    <legacyCorruptedStateExceptionsPolicy enabled="true" />

  </runtime>
  
  <userSettings>
        <XoKeyHostApp.Properties.Settings>
            <setting name="Debug" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="Socks_Port" serializeAs="String">
                <value>7777</value>
            </setting>
        </XoKeyHostApp.Properties.Settings>
    </userSettings>
</configuration>
